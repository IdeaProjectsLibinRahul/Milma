<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MLTextView">
        <attr name="ml_textview_font" format="enum">
            <enum name="geosans" value="0" />
            <enum name="metanoia" value="1" />
            <enum name="sansation" value="2" />
            <enum name="sansation_bold" value="3" />
            <enum name="normal" value="9" />
        </attr>
        <attr name="ml_textview_fontstyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="bolditalic" value="2" />
            <enum name="italic" value="3" />
            <enum name="light" value="4" />
        </attr>
    </declare-styleable>
</resources>